{
    "version": 2,
    "builds": [
      {
        "src": "src/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/index.js"
      },
      {
        "src": "/api/auth/v1/(.*)",
        "dest": "src/index.js"
      },
      {
        "src": "/api/user/v1/(.*)",
        "dest": "src/index.js"
      }

    ]
  }
  